with Ada.Strings;
with Ada.Strings.Fixed;
function SAL.Gen_Trimmed_Image (Item : in Element_Type) return String
is
   use Ada.Strings;
   use Ada.Strings.Fixed;
begin
   return Trim (Element_Type'Image (Item), Both);
end SAL.Gen_Trimmed_Image;
