(DEFINE (PROBLEM ATT-LOG0)
   (:DOMAIN LOGISTICS-STRIPS)
   (:OBJECTS PACKAGE1 PGH-TRUCK BOS-TRUCK AIRPLANE1 BOS-PO PGH-PO
             BOS-AIRPORT PGH-AIRPORT PGH BOS)
   (:INIT (OBJ PACKAGE1)
          (TRUCK PGH-TRUCK)
          (TRUCK BOS-TRUCK)
          (AIRPLANE AIRPLANE1)
          (LOCATION BOS-PO)
          (LOCATION PGH-PO)
          (LOCATION BOS-AIRPORT)
          (LOCATION PGH-AIRPORT)
          (AIRPORT BOS-AIRPORT)
          (AIRPORT PGH-AIRPORT)
          (CITY PGH)
          (CITY BOS)
          (IN-CITY PGH-PO PGH)
          (IN-CITY PGH-AIRPORT PGH)
          (IN-CITY BOS-PO BOS)
          (IN-CITY BOS-AIRPORT BOS)
          (AT PACKAGE1 PGH-AIRPORT)
          (AT AIRPLANE1 PGH-AIRPORT)
          (AT BOS-TRUCK BOS-PO)
          (AT PGH-TRUCK PGH-PO))
   (:GOAL (AND (AT PACKAGE1 BOS-AIRPORT))))