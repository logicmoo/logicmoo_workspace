(DEFINE (PROBLEM STRIPS-GRIPPER2)
   (:DOMAIN GRIPPER-STRIPS)
   (:OBJECTS ROOMA ROOMB BALL1 BALL2 LEFT RIGHT)
   (:INIT (ROOM ROOMA)
          (ROOM ROOMB)
          (BALL BALL1)
          (BALL BALL2)
          (GRIPPER LEFT)
          (GRIPPER RIGHT)
          (AT-ROBBY ROOMA)
          (FREE LEFT)
          (FREE RIGHT)
          (AT BALL1 ROOMA)
          (AT BALL2 ROOMA))
   (:GOAL (AND (AT BALL1 ROOMB)
               (AT BALL2 ROOMB))))